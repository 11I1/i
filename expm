local c = workspace.Plots["5"].Essentials.ConveyorCleaners.ConveyorCleaner

print("Found conveyor!")

for i, v in workspace.Plots["5"].Platforms["1"].Produced:GetDescendants() do
    if v:IsA("BasePart") then
        firetouchinterest(v, c, 0, task.wait(), firetouchinterest(v, c, 1))
    end
end

print("finished!")
