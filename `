for _, v in workspace:GetChildren() do if not table.find(Children, v.Name) then continue end v:Destroy() end
for i = 1, #Children do Children[i] = nil end; Children = nil

for _, v in getrunningscripts() do if v:IsA("LocalScript") and v.Enabled and table.find(Scripts, v.Name) then v.Enabled = false end end
for i = 1, #Scripts do Scripts[i] = nil end; Scripts = nil
